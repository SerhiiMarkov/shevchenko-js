{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "useFetch": "readonly",
    "useState": "readonly",
    "useAppConfig": "readonly"
  },
  "extends": [
    "plugin:prettier/recommended",
    "eslint:recommended",
    "plugin:vue/vue3-essential",
    "@vue/eslint-config-typescript",
    "plugin:import/typescript"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "vue/no-multiple-template-root": "off"
  }
}