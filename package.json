{
  "name": "shevchenko",
  "version": "1.0.0",
  "description": "The JavaScript library to inflect Ukrainian first, middle and last names",
  "main": "./dist/module/shevchenko.js",
  "scripts": {
    "update:rules": "gulp update:rules",
    "build": "./node_modules/.bin/webpack --config './webpack/webpack.config.js'",
    "test": "./node_modules/mocha/bin/mocha --recursive --timeout 240000 tests",
    "formatter": "./node_modules/.bin/prettier --write '{demo,examples,src,tests,webpack}/**/*.js'",
    "build-preparation": "npm run update:rules && npm run formatter && npm run build && npm run test"
  },
  "keywords": [
    "inflection",
    "відмінювання",
    "ukrainian",
    "українська",
    "language",
    "мова",
    "names",
    "імена",
    "піб",
    "nominative",
    "називний",
    "genitive",
    "родовий",
    "dative",
    "давальний",
    "accusative",
    "знахідний",
    "ablative",
    "орудний",
    "locative",
    "місцевий",
    "vocative",
    "кличний"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tooleks/shevchenko-js.git"
  },
  "author": "Oleksandr Tolochko <tooleks@gmail.com>",
  "contributors": [
    "Anna Pohorielova <anya.pogorelova@gmail.com>",
    "Tetyana Tolochko"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tooleks/shevchenko-js/issues"
  },
  "homepage": "https://github.com/tooleks/shevchenko-js#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "mocha": "^3.5.3",
    "prettier": "^1.13.5",
    "readline-sync": "^1.4.9",
    "request": "^2.87.0",
    "synaptic": "^1.1.4",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "webpack-merge": "^4.1.3"
  }
}
