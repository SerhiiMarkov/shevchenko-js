{
  "name": "shevchenko",
  "version": "1.1.6",
  "description": "The JavaScript library to inflect Ukrainian first, middle and last names",
  "main": "./dist/shevchenko.umd.js",
  "scripts": {
    "update:rules": "./node_modules/.bin/gulp update:rules",
    "train:nn-pos-a-ya": "node -r esm './nn/POS_A_YA/train.js'",
    "train:nn-pos-oi-yi-ii": "node -r esm './nn/POS_OI_YI_II/train.js'",
    "train:nn-pos-yh": "node -r esm './nn/POS_YH/train.js'",
    "build": "./node_modules/.bin/webpack -r esm --progress --config './webpack.config.js'",
    "test": "./node_modules/mocha/bin/mocha -r esm --recursive --bail './tests/main.js'",
    "format": "./node_modules/.bin/prettier --write '**/*.js' '**/*.mjs' '!dist/**'",
    "build:release": "npm run update:rules && npm run format && npm run build && npm run test"
  },
  "keywords": [
    "inflection",
    "відмінювання",
    "ukrainian",
    "українська",
    "language",
    "мова",
    "names",
    "імена",
    "піб",
    "nominative",
    "називний",
    "genitive",
    "родовий",
    "dative",
    "давальний",
    "accusative",
    "знахідний",
    "ablative",
    "орудний",
    "locative",
    "місцевий",
    "vocative",
    "кличний"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tooleks/shevchenko-js.git"
  },
  "author": "Oleksandr Tolochko <tooleks@gmail.com>",
  "contributors": [
    "Anna Pohorielova <anya.pogorelova@gmail.com>",
    "Tetyana Tolochko"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tooleks/shevchenko-js/issues"
  },
  "homepage": "https://github.com/tooleks/shevchenko-js#readme",
  "dependencies": {
    "synaptic": "^1.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "esm": "^3.1.3",
    "faker": "^4.1.0",
    "gulp": "^3.9.1",
    "mocha": "^3.5.3",
    "moment": "^2.24.0",
    "prettier": "^1.16.1",
    "request": "^2.88.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-plugin-replace": "^1.2.0"
  }
}
