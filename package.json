{
  "name": "shevchenko",
  "version": "1.2.0",
  "description": "JavaScript library to inflect Ukrainian first, middle and last names",
  "browser": "./dist/umd/shevchenko.js",
  "main": "./dist/cjs/shevchenko.js",
  "module": "./dist/es/shevchenko.js",
  "types": "./dist/types/shevchenko.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && rollup -c && tsc -p ./tsconfig.declaration.json",
    "lint": "tslint --fix -p ./tsconfig.json -c ./tslint.json",
    "test": "mocha --bail ./tests/entrypoint.js"
  },
  "keywords": [
    "inflection",
    "відмінювання",
    "ukrainian",
    "українська",
    "language",
    "мова",
    "names",
    "імена",
    "піб",
    "nominative",
    "називний",
    "genitive",
    "родовий",
    "dative",
    "давальний",
    "accusative",
    "знахідний",
    "ablative",
    "орудний",
    "locative",
    "місцевий",
    "vocative",
    "кличний"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tooleks/shevchenko-js.git"
  },
  "author": "Oleksandr Tolochko <tooleks@gmail.com>",
  "contributors": [
    "Anna Pohorielova <anya.pogorelova@gmail.com>",
    "Tetyana Tolochko"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tooleks/shevchenko-js/issues"
  },
  "homepage": "https://github.com/tooleks/shevchenko-js#readme",
  "dependencies": {
    "synaptic": "^1.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/lodash": "^4.14.150",
    "@types/synaptic": "^1.0.3",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "mocha": "^3.5.3",
    "rollup": "^2.7.1",
    "tslint": "^6.1.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.8.3"
  }
